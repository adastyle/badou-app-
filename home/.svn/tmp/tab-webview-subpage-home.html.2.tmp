									osg.open("propList.html",{"homestyleId":styleTypeId,"fig":"首页进入","title":title});
								}else{
									osg.ajax("/home/changeAndMore",{"id":styleTypeId,"tag":"change"},function(data){
										reLoadOnce1(data.homecontents);
									})
								}
							});
							var likes=videos[i].homecontents;
							for(var j=0;j<2;j++){
								var currImgShow=$(twoImgShow).clone();
								currImgShow.find(".bigImg").attr("src",qiniuRoot+likes[j].info.logo+ '!app.voicezheng.img');
