									currImgShow.find(".lock").hide();
								}else{
									if(likes[j].info.buyType==0)
										currImgShow.find(".lock").attr("src","../resources/images/vipShow.png").show();
									else if(likes[j].info.buyType==2)
										currImgShow.find(".lock").attr("src","../resources/images/moneyShow.png").show();
								}
								currImgShow.attr("data-id",likes[j].typeId);
								currImgShow.data('data', likes[j].info);
								if(likes[j].type==1){
										currImgShow.tap(function() {
											var voice_data = $(this).data('data');
											detailCklic(voice_data);
										});
									}else if(likes[j].type==2){
										currImgShow.tap(function() {
											var id = $(this).attr('data-id');
											osg.open("audioList.html",{"categoryId":id,"cateGoryInfo":"分类"});
