							}
							$(".allModule").append(module5);
						}
					}
				})
				//换一换相关方法
				function contensItem(oneData){
					var currImgShow=$(fourthImgShowAndTitle).clone();
					currImgShow.find(".bigImg").attr("src",qiniuRoot+oneData.info.logo+ '!app.voicezheng.img');
					currImgShow.find(".currMemo").html(oneData.info.memo);
					if(mui.os.ios && osg.settingValue('isPublish') == 1){
						currImgShow.find(".lock").hide();
					}else{
						if(oneData.info.buyType==0)
							currImgShow.find(".lock").attr("src","../resources/images/vipShow.png").show();
						else if(oneData.info.buyType==2)
							currImgShow.find(".lock").attr("src","../resources/images/moneyShow.png").show();
					}
					currImgShow.attr("data-id",oneData.typeId);
					currImgShow.data('data', oneData.info);
					if(oneData.type==1){
						currImgShow.find(".curTitle").html(oneData.info.title);
						currImgShow.tap(function() {
							var voice_data = $(this).data('data');
							detailCklic(voice_data);
						});
					}else if(oneData.type==2){
						currImgShow.find(".curTitle").html(oneData.info.name);
						currImgShow.tap(function() {
							var id = $(this).attr('data-id');
							osg.open("audioList.html",{"categoryId":id,"cateGoryInfo":"分类"});
						});
					}else{
						currImgShow.find(".curTitle").html(oneData.info.name);
						currImgShow.tap(function() {
							var id = $(this).attr('data-id');
							osg.open("audioList.html",{"categoryId":id,"cateGoryInfo":"资源包"});
						});
					}
					return currImgShow;
				}
				
				function openDetail(id,type){
					if('v'==type && id){
						osg.ajax('voice/findById',{'id':id},function(data){
								detailCklic(data);
						})
					}else if('z'==type){
						var currCateGory='资源包';
						if(id){
							osg.open('audioList.html', {
								"categoryId": id,
								"cateGoryInfo":currCateGory
							}, null, {
								im: 1
							});
						}
					}else if('s'==type){ //商品详情页
						if(id){
							osg.open('../mall/details.html', {
								"bookId": id
							}, null, {
								im: 1
							});
						}
					}else if('m'==type){ //少年绘
						osg.evtFireEvent('toMatch');
					}else if('d'==type){ //去登录页面
						if(osg.isLogined()) {
							osg.toast('你已经登录了！');
						} else {
							osg.open('../mine/login.html', {
							}, null, {
								im: 1
							});
						}
					}
				}
			});
			/*我正在听的更多点击*/
			$(".swiper-wrapper").on("tap",".goOnLook",function(){
				osg.open("listening.html");
			})
			$(".currLinterMore").tap(function(){
				osg.open("listening.html");
			})
			/*十万个为什么的全部播放*/
			$(".allAudioPlay").tap(function(){
				playAll(whys);
			})
			/*全部播放*/
			function playAll(whys){
				if(!whys.length) return;
				osg.set('voice_list',whys);//把列表缓存到本地
				osg.set("voice_now",whys[0]);
				osg.evtFireEvent('update');
			}
			//去详情页
			function detailCklic(voice_data){
				if(voice_data.buyType==1){
					var list = [voice_data];
					osg.set('voice_list',list);//把列表缓存到本地
					osg.set("voice_now",voice_data);
					var detailsView = plus.webview.getWebviewById("details");
					if(detailsView){
						osg.evtFireEvent('update');
					}
					osg.open('audioPlay.html', {
					}, null, {
						im: 1,
						id:"details"
					});
				}else{
					if(voice_data.resourcePackId){
						osg.open('audioList.html', {
							"categoryId": voice_data.resourcePackId,
							"cateGoryInfo":"资源包"
						}, null, {
							im: 1
						});
					}
				}
				
			}
			
			/*热听榜*/
			$(".hotMusic").tap(function(){
				osg.open('hotListen.html', {
					"voiceInfo":"热听榜"
				}, null, {
					im: 1
				});
			});
			/*答题闯关*/
			$(".answer").tap(function(){
				if(osg.isLogined()) {
					osg.ajax('form/formcheked', {}, function(data) {
						if(data){
							osg.open('../juvenile/testResult.html', {
								"enrollPhaseId":"111111",
								"times":0
							}, null, {
								im: 1
							});
						}else{
							osg.open('../juvenile/onlineAnswer.html', {
								"enrollPhaseId":"111111"
							}, null, {
								im: 1,
								id:"onlineAnswer.html"
							});
						}
						
						
					}, {
						noload: true
					});
					
				} else {
					osg.open('../mine/login.html', null, null, {
						im: 1
					});
					
				}
				
			});
			$(".search").tap(function(){
				event.stopPropagation(); // 禁止事件冒泡到父节点点击事件
				osg.open("../home/audioSearch.html");
			});
			/*十万个为什么的查看更多模块*/
			$(".moreRecommendAudio").tap(function(){
				osg.open("moreWhy.html");
			})
			
			/*屏幕滑动*/
			mui('.mui-scroll-wrapper').scroll({
				indicators: true
			});
