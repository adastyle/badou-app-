/*支付组件样式开始*/
.pay_menu {
	display: none;
	background-color: hsla(0, 0%, 0%, 0.5);
}

.pay_menu .pay_bar {
	position: absolute;
	bottom: 0;
	left: 0;
	right: 0;
	z-index: 10000;
	background-color: #fff;
	padding: 0 1.5rem;
	height: 8rem;
}

.pay_menu .pay_bar .pay_btn {
	display: inline-block;
	width: 49%;
	text-align: center;
	height: 100%;
	padding-top: 2.5rem;
}

.pay_menu .pay_bar .pay_btn i {
	font-size: 2.6rem;
}

.pay_menu .pay_bar .pay_btn div {
	margin-top: 0.5rem;
