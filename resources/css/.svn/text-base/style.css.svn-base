/*支付组件样式结束*/
.addressWindow,
.shadow{
	position: fixed;
	top:0;
	height:100%;
	width:100%;
	background:rgba(0,0,0,0.5);
	text-align: center;
	z-index:1000;
	list-style: none;
	display:none;
	transition: all .5s;
}
/*线性渐变按钮*/
.linearGradientBtn{
	display:inline-block;
	width:70%;
	height:2.5rem;
	background:linear-gradient(90deg,rgba(249,94,87,1),rgba(211,36,36,1));
	border-radius:45px;
	text-align: center;
	line-height:2.5rem;
	color:#FFFFFF !important;
	font-size:1.2rem;
}

/*显示音频列表*/
.audioListShadow{
	position:fixed;
	top:0;
	left:0;
	z-index:9999;
	background:rgba(0,0,0,0.2);
	width:100%;
	height:100%;
	transition: all 1s;
	display:none;
}
.audioListShadow>div:first-child{
	width:100%;
	height:50%;
	background:#fff;
	position: absolute;
	bottom:3rem !important;
	left:0;
	overflow: auto;
}

.audioListShadow .closeList{
	height:3.5rem;
	width:100%;
	line-height: 3.5rem;
	text-align: center;
	color:#121212;
	border-top:1px solid #f5f6f7;
	position: absolute;
	left:0;
	z-index:10001;
	background:#fff;
	bottom:0;
	margin-bottom:0;
}
.currPlay{
	height:3.5rem;
	line-height:3.5rem;
	width:100%;
	color:#121212;
	border-bottom:1px solid #f5f6f7;
	position: absolute;
	left:0;
	z-index:10001;
	background:#fff;
	bottom:54%;
	padding-left:1rem;
}
.currPlay .playing{
	float:right;
	font-size:1.2rem;
	color:#D32424;
	padding:0 1rem;
}
.voiceOne_title{
	display:inline-block;
	width:70%;
	vertical-align: middle;
	font-size:.9rem;
}
.audioListShadow ul>li{
	margin:1rem;
	position:relative;
}
.audioListShadow ul>li .delete{
	position:absolute;
	top:50%;
	right:-1rem;
	transform:translateY(-50%);
	font-size:.8rem;
}
.audioListShadow .mui-scroll-wrapper{
	top:auto;
	bottom:0 !important;
}
.audioListShadow>div>ul img{
	width:2.5rem;
	height:2.5rem;
	border-radius:50%;
	vertical-align: middle;
	margin-right:.3rem;
}
.delete{
	color:#C8CCCC;
	display:inline-block;
	width:3rem;
	height:100%;
	text-align: center;
	padding-top:.5rem;
}
.jp-playlist{
	padding:0 1rem;
}
.currPlayActive span{
	color:#FFAA33;
}
/*显示播放*/
.bottomFixed{
	position: fixed;
	bottom:0;
	height:3.2rem;
	width:100%;
	z-index:1000;
	box-shadow:0px 3px 22px 2px rgba(239,239,239,0.97);
