/*调试状态样式inherit，非调试状态时display:none*/

.debug {
	display: inherit;
}

/*通用元素样式*/

html {
	font-size: 16px !important;
	/*overflow-x: hidden;*/
}
/*分割线*/
.splitLine{
	height: 6px;
	margin-left:-3%;
	margin-right:-3%;
	background:#F5F5F5;
}

body {
	background-color: #fff;
	margin: 0;
	padding: 0;
	font-size: 1rem;
	display: inline-block;
	overflow-x: hidden;
	width: 100%;
}

p {
	font-size: inherit;
	color: inherit;
}

pre {
	display: inline-block;
	margin: 0;
}

div {
	vertical-align: top;
}

a {
	text-decoration: none;
}

body>.mui-progressbar {
	top: 2.8rem;
}

ul {
	padding: 0;
	margin: 0;
}

li {
	list-style-position: inside;
	font-size: 1rem;
	list-style-type: none;
}

.mui-content {
	/*height: 100%;*/
	bottom: 0;
	background-color: #f0f4f6;
}

.mui-input-group {
	margin-top: 10px;
}

.mui-input-group:first-child {
	margin-top: 20px;
}

a.mui-action-back {
	color: #231815;
}

.mui-bar-nav~.mui-content {
	margin-top: 2.8rem;
	padding-top: 0;
}

.mui-bar~.mui-scroll-wrapper {
	margin-top: 2.8rem;
}

.mui-switch {
	width: 62px;
}

.mui-switch.mui-active:before {
	left: 10px;
	content: '是';
}

.mui-switch:before {
	top: 6px;
	content: '否';
}

.mui-switch.mui-active {
    border: 2px solid rgba(211, 58, 49, 1);
    background-color: rgba(211, 58, 49, 1);
}

/*通用样式定义*/
.hidden {
	display: none !important;
}

.disabled {
	color: #D5D5D5 !important;
}

input:disabled,input[disabled]{
	color: #D5D5D5 !important;
}

.inline {
	display: inline-block;
}

.absolute {
	position: absolute;
	left: 0;
	top: 0;
}

.more {
	text-overflow: ellipsis;
	overflow: hidden;
	white-space: nowrap;
}

.more2 {
	overflow: hidden;
	display: -webkit-box !important;
	-webkit-line-clamp: 2;
	-webkit-box-orient: vertical;
	word-break: break-all;
	white-space: normal !important;
}

.more3 {
	overflow: hidden;
	display: -webkit-box !important;
	-webkit-line-clamp: 3;
	-webkit-box-orient: vertical;
	word-break: break-all;
	white-space: normal !important;
}

.more4 {
	overflow: hidden;
	display: -webkit-box !important;
	-webkit-line-clamp: 4;
	-webkit-box-orient: vertical;
	word-break: break-all;
	white-space: normal !important;
}

.more5 {
	overflow: hidden;
	display: -webkit-box !important;
	-webkit-line-clamp: 5;
	-webkit-box-orient: vertical;
	word-break: break-all;
	white-space: normal !important;
}

.more6 {
	overflow: hidden;
	display: -webkit-box !important;
	-webkit-line-clamp: 6;
	-webkit-box-orient: vertical;
	word-break: break-all;
	white-space: normal !important;
}

.m-l-4 {
	margin-left: 0.25rem;
}

.m-l-16 {
	margin-left: 1rem;
}

.m-t-10 {
	margin-top: 0.625rem;
}

/* checkbox checkbox样式*/
.mui-checkbox, .mui-radio {
	display: inline-block;
}

.mui-checkbox.mui-left input[type=checkbox],
.mui-radio.mui-left input[type=radio] {
	left: 0;
	width: auto;
	height: auto;
}

.mui-checkbox.mui-left label,
.mui-radio.mui-left label {
	padding-right: 0.375rem;
	padding-left: 2.125rem;
}

.mui-checkbox input[type=checkbox]:checked:before, .mui-radio input[type=radio]:checked:before {
	color: #008FD3;
}

.mui-checkbox input[type=checkbox], .mui-radio input[type=radio] {
	top: 0.125rem;
}

/*titlebar相关样式*/

.mui-title {
	color: #fff;
	font-size: 1.11765rem;
	height: 2.8rem;
	line-height: 2.8rem;
	font-family: "苹方";
}

.mui-bar-nav {
	background: #fff;
    border-bottom:1px solid transparent;
    padding-top:1rem;
    padding-left:0
}

.bar_input {
	position: absolute;
    left: 4.75rem;
    bottom: 0.8rem;
    right: 5.8rem;
    height: 1.75rem;
}

.bar_input>input {
	border: #D66059;
    font-size: 0.825rem;
    height: 1.75rem;
    background: #D66059;
    padding-left: 1.5rem;
    border-radius: 5px;
}
::-webkit-input-placeholder { /* WebKit, Blink, Edge */
    color:    #E5E5E5;
}
:-moz-placeholder { /* Mozilla Firefox 4 to 18 */
   color:    #E5E5E5;
   opacity:  1;
}
::-moz-placeholder { /* Mozilla Firefox 19+ */
   color:    #E5E5E5;
   opacity:  1;
}
:-ms-input-placeholder { /* Internet Explorer 10-11 */
   color:    #E5E5E5;
}
::-ms-input-placeholder { /* Microsoft Edge */
   color:    #E5E5E5;
}

.bar_message_btn {
	position: absolute;
	bottom: 0.5rem;
	right: 0.5rem;
	height: 1.875rem;
	width: 2.25rem;
	text-align: center;
}

.bar_message_btn i {
	font-size: 1.375rem;
	line-height: 1.875rem;
	color: #fff;
}


/*tabbar相关样式*/

.mui-bar-tab {
	background-color: #fff;
	height: 3rem !important;
}

.mui-bar {
	-webkit-box-shadow: 0 0 1px #d6d6d6;
	box-shadow: 0 0 1px #d6d6d6);
	height: auto;
	font-size:1.1rem;
}

.mui-bar-tab .mui-tab-item {
	height: auto;
	color: #929292 !important;
	padding-top: 2px;
}
.mui-bar-tab .mui-tab-item.mui-active {
	color: #D32424 !important;
}

.mui-bar-tab .mui-tab-item .iconfont~.mui-tab-label {
	font-size: 0.625rem;
	display: block;
	overflow: hidden;
	text-overflow: ellipsis;
	margin-bottom: -0.1rem;
	line-height: 18px;
}

.mui-bar-tab .mui-tab-item .iconfont {
	font-size: 1.25rem;
	padding-top: 0;
	padding-bottom: 0;
}

/*.mui-action-back {
	width: 2.8rem;
	text-align: center;
}*/

.mui-action-back {
	line-height: 2.8rem;
	height: 2.8rem;
	font-size: 1.5rem;
	margin-left:.7rem;
}
.mui-action-back+p{
	display:inline-block;
	text-align: center;
	width:85%;
	font-weight: bold;
}

.mui-bar .iconfont {
	z-index: 20;
}

.iconfont .mui-badge {
	font-size: 10px;
	line-height: 1.4;
	position: absolute;
	top: -2px;
	left: 100%;
	margin-left: -10px;
	padding: 1px 5px;
	color: #fff;
	background: red;
}

/*下拉刷新、上拉加载列表样式*/

.mui-bar~.mui-content .mui-fullscreen {
	top: 3.4rem !important;
	height: auto;
}

.mui-pull-top-tips {
	position: absolute;
	top: -20px;
	left: 50%;
	margin-left: -25px;
	width: 40px;
	height: 40px;
	border-radius: 100%;
	z-index: 1;
}

.mui-bar~.mui-pull-top-tips {
	top: 24px;
}

.mui-pull-top-wrapper {
	width: 42px;
	height: 42px;
	display: block;
	text-align: center;
	background-color: #efeff4;
	border: 1px solid #ddd;
	border-radius: 25px;
	background-clip: padding-box;
	box-shadow: 0 4px 10px #bbb;
	overflow: hidden;
}

.mui-pull-top-tips.mui-transitioning {
	-webkit-transition-duration: 200ms;
	transition-duration: 200ms;
}

.mui-pull-top-tips .mui-pull-loading {
	/*-webkit-backface-visibility: hidden;
					-webkit-transition-duration: 400ms;
					transition-duration: 400ms;*/
	margin: 0;
}

.mui-pull-top-wrapper .mui-icon,
.mui-pull-top-wrapper .mui-spinner {
	margin-top: 7px;
}

.mui-pull-top-wrapper .mui-icon.mui-reverse {
	/*-webkit-transform: rotate(180deg) translateZ(0);*/
}

.mui-pull-bottom-tips {
	text-align: center;
	background-color: #efeff4;
	font-size: 15px;
	line-height: 40px;
	color: #777;
}

.mui-pull-top-canvas {
	overflow: hidden;
	background-color: #fafafa;
	border-radius: 40px;
	box-shadow: 0 4px 10px #bbb;
	width: 40px;
	height: 40px;
	margin: 0 auto;
}

.mui-pull-top-canvas canvas {
	width: 40px;
}

.mui-pull-bottom-wrapper {
	background: #fff;
}

.mui-slider .mui-slider-group .mui-slider-item .logo_img {
	width: 30%;
}


/*下拉刷新、上拉加载列表样式结束*/




/*新闻页分类横向滚动条相关样式*/

.mui-segmented-control.mui-segmented-control-inverted .mui-control-item.mui-active {
	color: #D33A31;
}

.mui-segmented-control.mui-segmented-control-inverted .mui-control-item.mui-active {
	border-bottom: 1px solid #D33A31;
}

.mui-segmented-control {
	font-size: 0.9rem;
	background-color: #fff;
}

.mui-slider .mui-segmented-control.mui-segmented-control-inverted~.mui-slider-group .mui-slider-item {
	border: 0;
}

.mui-fullscreen .mui-segmented-control~.mui-slider-group {
	top: 2.2rem;
}

.mui-segmented-control.mui-scroll-wrapper {
	height: 3.6rem;
}

.mui-segmented-control.mui-scroll-wrapper .mui-scroll {
	height: 2.1rem;
}

.mui-segmented-control .mui-control-item {
	font-size: 1rem;
	line-height: 2.1rem;
}

.mui-segmented-control.mui-scroll-wrapper .mui-control-item {
	    padding: 0.7rem 1rem;
}

/*轮播区域样式*/

.mui-fullscreen.mui-slider .mui-slider-item>a {
	top: 0;
	-webkit-transform: none;
	transform: none;
}

.mui-slider-item>a>.img_banner {
	width: 100%;
	/*height: 11rem;*/
}

.mui-slider-item>a>div {
	position: absolute;
	bottom: 0rem;
	left: 0;
	width: 100%;
	background-color: #000;
	opacity: 0.8;
	height: 30px;
}

.mui-slider-title {
	line-height: 30px;
	height: 30px;
	margin: 0;
	color: #fff;
	text-align: left;
	text-indent: 12px;
	width: 80%;
	display: block;
	overflow: hidden;
	/*单行*/
	white-space: nowrap;
	-o-text-overflow: ellipsis;
	text-overflow: ellipsis;
}

.slider-indicator {
	position: absolute;
	bottom: 0;
	width: 100%;
	text-align: center;
	background: rgba(0, 0, 0, 0.4) 0 0;
	height: 40px;
	line-height: 40px;
}

.slider-indicator>div:first-child {
	text-align: left;
	width: 80%;
	float: left;
	color: #ffffff;
	font-size: 13px;
	margin-left: 6px;
}

.mui-indicator.mui-active {
	background: #1790d0!important;
}

.mui-slider-indicator .mui-indicator {
	display: inline-block;
	width: 6px;
	height: 6px;
	margin: -1px 6px;
	cursor: pointer;
	border-radius: 0;
	background: #aaa;
	-webkit-box-shadow: 0 0 1px 1px rgba(130, 130, 130, .7);
	box-shadow: 0 0 1px 1px rgba(130, 130, 130, .7);
	z-index: 999;
}

.mui-slider-indicator .indicator-margin {
	margin-left: 0.05rem;
}


