/*jPlayer样式*/
.jp-container {
	position: relative;
}

.jp-controll-div {
	position: absolute;
	width: 100%;
	top: 0;
	bottom: 0;
	background-color: rgba(0, 0, 0, 0.4392156862745098);
	text-align: center;
	color: #FCFAFB;
}

.jp-controll-div .jp-progress .jp-seek-bar {
	width: 100%;
	height: 2px;
	background-color: #70635A;
}

.jp-controll-div .jp-progress .jp-seek-bar .jp-play-bar {
	height: 100%;
	background-color: #B05854;
	position: relative;
	overflow: visible !important;
	/*background-color: #E8E2D6;*/
}

.jp-play-bar-dot {
	width: 16px;
	height: 16px;
	border-radius: 8px;
	background-color: #FCFAFB;
	position: absolute;
	right: -10px;
	top: -7px;
}

.jp-controll-div .jp-bottom-bar {
	width: 100%;
	bottom: 16px;
	position: absolute;
}

.jp-controll-div .jp-progress {
	position: absolute;
	left: 62px;
	right: 106px;
	top: 9px;
}

.jp-play {
	display: inline-block;
	margin: 0 auto;
	margin-top: 24%;
}

.jp-current-time {
	display: inline-block;
	float: left;
	margin-left: 16px;
	font-size: 0.875rem;
}

.jp-bottom-bar-right {
	float: right;
	margin-right: 16px;
}

.jp-duration {
	margin-right: 20px;
	display: inline-block;
	font-size: 0.875rem;
}

.jp-full-screen {
	display: inline-block;
}

.jp-full-screen>i {
	font-size: 1.2rem;
}

.jp-play:before {
	color: #FCFAFB;
	font-size: 2rem;
	content: "\e618";
}

.jp-state-playing .jp-play:before {
	content: "\e696";
}

/*轮播样式*/
.xyBanner .mui-scroll-wrapper .mui-slider{
	/*height:9rem;*/
	margin-top:.6rem;
	border-radius:0px;
	margin-bottom:1rem;
}
.xyBanner .mui-scroll-wrapper .mui-slider .mui-slider-item>a{
	display:block;
}
.xyBanner .mui-scroll-wrapper .mui-slider .mui-slider-item>a>img{
	/*height:9rem;*/
	width:100%;
}
/*轮播的小圆点*/
.xyBanner .mui-slider-indicator{
	padding-right:1.3rem;
	bottom:0;
}
.xyBanner .mui-slider-indicator .mui-indicator{
	width:25px;
	height:6px;
	background:rgba(210,213,213,0);
	border:1px solid rgba(242, 242, 242, 1);
	border-radius:3px;
